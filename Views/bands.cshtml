<h2>Bands:</h2>
<ul>
  @if (Model.Count != 0)
  {
    @foreach (var band in Model)
    {
    <li><a href="/band/@band.GetId()">@band.GetName()</a></li>
    }
  }
</ul>



<h4><a href="/bands/new">Add a new band</a></h4>



<h4><a href="/">Return home</a></h4>

<form action="/bands/clear" method="post">
  <button type="submit">Clear all bands</button>
</form>
